{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/client/index.html"
        }
    ],
    "functions": {
        "api/index.ts": {
            "includeFiles": "server/data/**"
        }
    }
}